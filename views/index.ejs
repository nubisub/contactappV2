
<div class="mx-16 my-12 ">
    <!-- add Button -->
    <div class="pb-4">
        <button class="p-2 px-4 bg-emerald-500 text-white hover:bg-emerald-400  rounded ">Add Contact</button>
    </div>
    <!-- Table -->
    <div class="flex flex-row ">
        <!-- Nama -->
        <div class="min-w-[250px] bg-gray-100 flex items-center  border-solid border-2 p-4 py-2">Nama</div>
        <!-- Email -->
        <div class="min-w-[250px] bg-gray-100  flex items-center  border-solid border-2 p-4 py-2 border-l-0 ">Email </div>
        <!-- Phone -->
        <div class="min-w-[250px] bg-gray-100  flex items-center  border-solid border-2 p-4 py-2 border-l-0 ">Phone Number </div>
        <!-- Action -->
        <div class="min-w-[200px]  bg-gray-100 flex items-center  border-solid border-2 p-4 py-2 pl-6 border-l-0">
            Action
        </div>
    </div>

<% data.forEach(element => { %>
    <div class="flex flex-row">
        <!-- Nama -->
        <div class="min-w-[250px] flex items-center  border-solid border-2 p-4 border-t-0"><%= element.nama %> </div>
        <!-- Email -->
        <div class="min-w-[250px]  flex items-center   border-solid border-2 p-4 border-l-0 border-t-0" ><%= element.email  %> </div>
        <!-- Phone -->
        <div class="min-w-[250px]  flex items-center   border-solid border-2 p-4 border-l-0 border-t-0" ><%= element.phone  %> </div>
        <!-- Action Button -->
        <div class="flex p-4 py-2 justify-around items-center min-w-[200px]   border-solid border-2 p-4 border-l-0 border-t-0 ">
            <button class="p-2 px-4 bg-cyan-500 text-white rounded hover:bg-cyan-400">Edit</button>
            <button class="p-2 px-4 bg-red-600 text-white hover:bg-red-500  rounded ">Delete</button>
        </div>
    </div>
<% }); %>

</div>
